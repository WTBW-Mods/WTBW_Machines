{
  "multipart": [
    {
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_mk2"}
    },
    {
      "when" : { "north": "true" },
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_part_mk2" }
    },
    {
      "when" : { "south": "true" },
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_part_mk2", "y": 180 }
    },
    {
      "when" : { "west": "true" },
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_part_mk2" , "y": 270 }
    },
    {
      "when" : { "east": "true" },
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_part_mk2", "y": 90 }
    },
    {
      "when" : { "up": "true" },
      "apply": { "model": "wtbw_machines:block/cable/energy_cable_part_mk2", "x": 270 }
    },
    {
      "when" : { "down": "true" },
